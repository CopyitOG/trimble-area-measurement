{
  "name": "trimble-connect-sdk",
  "version": "4.0.9",
  "description": "Trimble Connect SDK for JavaScript",
  "homepage": "http://connect.trimble.com",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "keywords": [
    "trimble",
    "trimbleconnect",
    "organizer",
    "pset",
    "tcps"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "index.d.ts",
  "license": "SEE LICENSE IN https://community.trimble.com/docs/DOC-10021",
  "dependencies": {
    "atob": "^2.1.2",
    "browser-or-node": "^1.3.0",
    "fetch-blob": "^2.1.1",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@types/atob": "^2.1.1",
    "@types/browser-or-node": "^1.2.0",
    "@types/chai": "^4.3.0",
    "@types/chai-spies": "^1.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^18.11.9",
    "@types/ws": "^6.0.1",
    "aws-config": "^1.4.0",
    "aws-sdk": "^2.1035.0",
    "chai": "^4.3.3",
    "chai-spies": "^1.0.0",
    "guid-typescript": "^1.0.9",
    "isomorphic-fetch": "^3.0.0",
    "mocha": "^9.2.2",
    "mocha-bamboo-reporter": "^1.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "~4.7.4"
  },
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:es": "tsc -p tsconfig.es.json",
    "build:typedefs": "tsc --outDir . --emitDeclarationOnly",
    "build": "npm run build:cjs && npm run build:es && npm run build:typedefs",
    "clean": "rimraf -rf *.d.ts dist",
    "format": "tslint --fix \"./{src,test}/**/*.ts\"",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm test",
    "postpublish": "npm run clean",
    "test": "mocha"
  },
  "mocha": {
    "spec": "./test/**/*.spec.ts",
    "slow": 1500,
    "recursive": true,
    "timeout": "200000",
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "isomorphic-fetch",
      "fetch-blob",
      "chai",
      "chai-spies"
    ]
  }
}
